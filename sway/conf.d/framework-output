#set $midmon DP-9
#set $dualup DP-10
set $laptop eDP-1

output $dualup resolution 2560x2880 position 0 0 scale 1
#output $dualup resolution 2560x2880 position 0 1504 scale 1
output $midmon resolution 2560x1440 position 2560 720 scale 1 
